//ACID!  Move the slider

setCps(130/60/4)
$: s("hh!16").bank("tr808")
._scope()
$: s("sbd!4")._scope().duck("2:3:4").duckattack(.2).duckdepth(.8)
$bass: n(irand(10).sub(7).seg(16)).scale("a:minor")
.rib(46,1)
  .distort("2.2:.3 ")
.s("sawtooth").lpf(200).lpenv(slider(0.064,0,8)).lpq(12).orbit(2)
.lpq(12).orbit(2)._pianoroll()

$: s("supersaw").detune(1).rel(5).beat(2,32).slow(2).orbit(3)
.fm("2").fmh(2.04).room(1).roomsize(6)._pianoroll()

$: s("pulse").orbit(4).seg(16).dec(.1).fm(time).fmh(time).room(1).roomsize(6)._pianoroll()
